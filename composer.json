{
	"name" : "n2n/n2n-util-ical",
	"type" : "library",
	"description" : "ical utils",
	"keywords" : [
		"n2n",
		"ical"
	],
	"homepage" : "https://n2n.rocks/",
	"license" : "LGPL-3.0-or-later",
	"minimum-stability" : "dev",
	"prefer-stable": true,
	"config" : {
		"vendor-dir" : "src/vendor",
        "allow-plugins": {
          "n2n/n2n-composer-module-installer": true
        }
	},
	"require" : {
       "n2n/n2n-composer-module-installer" : "^7.1",
       "n2n/n2n-util": "^7.4"
    },
	"autoload" : {
		"psr-4" : {
			"n2n\\util\\ical\\" : "src/app/n2n/util/ical"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"n2n\\util\\ical\\": "src/test/n2n/util/ical"
		}
	},
	"require-dev" : {
		"phpunit/phpunit" : "^9.6"
	}
}